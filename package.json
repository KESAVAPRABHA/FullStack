        "jsonwebtoken": "^9.0.2",
        "mongodb": "^3.6.3",
        "mongoose": "^5.11.7",
        "nodemailer": "^6.9.15",
        "nodemon": "^3.1.7",
        "socket.io": "^3.0.4",
        "test": "^3.3.0",
        "yarn": "^1.22.10"
    }
